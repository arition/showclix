{
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [{
        "matches": [
            "https://www.showclix.com/event/*"
        ],
        "js": [
            "inject.js"
        ]
    }],
    "description": "showclix",
